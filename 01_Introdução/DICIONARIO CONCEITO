paises = dict(
    br = 'Brasil',
    py = 'Paraguay',
    ar = 'Argentina')
)

# para acrescentar

paises['us'] = 'Estados Unidos' # quando existe, atualiz. quando nao existe, cria
paises['br'] = 'Leo'
paises.update({'us' : 'Estados Unidos'})
print(paises)


